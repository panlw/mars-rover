{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "ecmaFeatures": {
        "jsx": false,
        "modules": true
    },
    "env": {
        "browser": false,
        "node": true
    },
    "rules": {
        "quotes": [
            2,
            "single"
        ]
    }
}
